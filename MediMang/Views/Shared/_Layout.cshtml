<!DOCTYPE html>
<html ng-app="app">
<head>
    <meta charset="utf-8" />
    <base href="/" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/bundles/css/plugins")
</head>
<body class="hold-transition skin-blue fixed sidebar-mini">
<div class="wrapper" ng-controller="LayoutController">
    @Html.Partial("_Header")


    @Html.Partial("_Sidebar")
    @*@Html.Partial("~/Views/Shared/_MenuNav.cshtml")*@
    <div  class="content-wrapper" block-ui="main">
        @RenderBody()
    </div>
</div>
    <div growl></div>
    <script src="//cdnjs.cloudflare.com/ajax/libs/moment.js/2.11.2/moment.min.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/fullcalendar/2.4.0/fullcalendar.min.js"></script>
    @Scripts.Render("~/bundles/angular")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/app")
    @Scripts.Render("~/bundles/plugins")
    <script src="/bundles/templates"></script> 
    @*@Scripts.Render("~/bundles/theme")*@
    @RenderSection("scripts", required: false)
</body>
</html>
